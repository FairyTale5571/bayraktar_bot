                            "type": "string"
                        },
                        "url": {
                            "type": "string"
                        }
                    }
                },
                "color": {
                    "type": "integer"
                },
                "description": {
                    "type": "string"
                },
                "fields": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "name": {
                                "type": "string"
                            },
                            "value": {
                                "type": "string"
                            }
                        }
                    }
                },
                "footer": {
                    "type": "object",
                    "properties": {
                        "icon_url": {
                            "type": "string"
                        },
