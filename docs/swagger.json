                },
                "lastUpdate": {
                    "type": "string"
                },
                "localize": {
                    "type": "string"
                },
                "maxPrice": {
                    "type": "integer"
                },
                "minPrice": {
                    "type": "integer"
                },
                "price": {
                    "type": "integer"
                },
                "randomDownPrice": {
                    "type": "boolean"
                },
                "randomMax": {
                    "type": "integer"
                },
                "randomMin": {
                    "type": "integer"
                },
                "resourceName": {
                    "type": "string"
                }
            }
        },
        "models.Embed": {
            "type": "object",
            "properties": {
                "author": {
                    "type": "object",
