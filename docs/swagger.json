        "models.Embeds": {
            "type": "object",
            "properties": {
                "content": {
                    "type": "string"
                },
                "embeds": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/models.Embed"
                    }
                }
            }
        },
        "models.Error": {
            "type": "object",
            "properties": {
                "error": {
                    "type": "string"
                }
            }
        }
    }
}